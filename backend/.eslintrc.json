{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-var":"error",
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "linebreak-style": 0,
      "no-shadow": "off"
    }
}
